{
  "name": "angular-template",
  "version": "0.0.1",
  "engine": {
    "node": "8.9.4",
    "npm": "5.6.0"
  },
  "scripts": {
    "build-api": "tsc",
    "api": "tsc -w | nodemon --inspect ./dist/app.js --watch dist",
    "build": "npm run build-api && npm run build-web",
    "start": "npm run build && node ./dist/app.js",
    "ng": "ng",
    "web": "ng serve -o",
    "build-web": "ng build ",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prem:build": "npm run server | npm run client ",
    "client": "http-server -p 8087 ./dist/public/",
    "server": "node ./dist/app.js",
    "prem:test": "ng serve | npm run api"
  },
  "private": true,
